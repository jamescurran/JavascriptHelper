@using NovelTheory.Component
@{
	var JScript = JavascriptHelper.Create(this);
}
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	@JScript.InsertCss()
</head>
<body>
	@RenderBody()

@JScript.InsertScripts()
@JScript.InsertOnReady()
</body>
</html>
